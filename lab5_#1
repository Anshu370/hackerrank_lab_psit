#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int a, b;
    char c;

    scanf("%d %d \n %c", &a, &b, &c);
    
    switch(c){
            
        case '+':
                printf("%d", a+b);
                break;
        
            
        case '-':
                printf("%d", a-b);
                break;
            
        case '*':
                printf("%d", a*b);
                break;
            
        case '/':
                printf("%d", a/b);
                break;
            
        default:
            printf("Invalid Choice");
             
    }

    return 0;
}
