#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

     int year;
    scanf("%d", &year);
    
    if(year%100!=0 && year%4==0){
        printf("YES");
    }
    else if(year%400==0){
        printf("YES");
    }
    else{
        printf("NO");
    }  
    return 0;
}
