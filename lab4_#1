#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char ch;
    
    scanf("%c", &ch);
    
    if((ch>='A' && ch<='Z') || (ch>='a' && ch<='z')){
        if(ch=='a' || ch=='e' || ch=='i'|| ch=='o'|| ch=='u' || ch=='A' || ch=='E' || ch=='I'|| ch=='O'|| ch=='U'){
            printf("VOWEL");
        }
        else{
            printf("CONSONANT");
        }
    }
    
    else if(ch>='0' && ch<='9'){
        printf("DIGIT");
    }
    else{
        printf("SPECIAL SYMBOL");
    }
        
    return 0;
}
